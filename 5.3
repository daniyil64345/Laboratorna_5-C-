#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;

int main() {
    double a, b, h, x, y;

    cout << "The beginning of the segment a: ";
    cin >> a;
    cout << "End of segment b: ";
    cin >> b;
    cout << "Step h: ";
    cin >> h;

    cout << "\n Function value table y = x^(3/4) + 6\n";
    cout << "--------------------------------------\n";
    cout << setw(10) << "x" << setw(15) << "y\n";
    cout << "--------------------------------------\n";

    for (x = a; x <= b; x += h) {
        if (x < 0) { 
            cout << "x = " << x << " Does not belong to ODZ (x >= 0)" << endl;
            break;  
        }

        y = pow(x, 3.0 / 4.0) + 6;
        cout << setw(10) << fixed << setprecision(2) << x
            << setw(15) << fixed << setprecision(4) << y << endl;
    }

    cout << "--------------------------------------\n";
    return 0;
}
