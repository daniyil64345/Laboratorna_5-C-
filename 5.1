#include <iostream>
#include <cmath>   
using namespace std;

int main() {
    double x, y;

    cout << "Write 5 different x:\n";

    for (int i = 1; i <= 5; i++) {
        cout << "x" << i << " = ";
        cin >> x;
        y = sin(5 * x) + 1.5 * x - 5;
        cout << "y" << i << " = " << y << endl;
    }

    return 0;
}
